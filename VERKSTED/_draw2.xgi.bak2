p1=0
p2=mellom_b


ruter=inndel_h-1
timesb=subtype+1

do until ruter=0
p1=0
p2=mellom_b

 do until timesb=0
 StartPolyline p1 hoyde/inndel_h*ruter
 EndLine p2 hoyde/inndel_h*ruter
 p1=p2+midtstokk
 p2=p1+mellom_b
 timesb=timesb-1
 end
