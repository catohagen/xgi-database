ruter=inndel_h-1
DrawRectBox 0 0 bredde hoyde

midtstokk=5/scale

p1=bredde-(midtstokk*subtype)
p1=p1/(subtype+1)
p2=p1+midtstokk

subtype2=subtype
do until subtype2=0

/*midtstokk*/
SetLineWidth 2
StartPolyline p1 0
EndLine p1 hoyde
StartPolyline p2 0
EndLine p2 hoyde

p3=p2+p1
p4=p3+midtstokk


p1=p3
p2=p4
subtype2=subtype2-1
end
SetLineWidth 1



if inndel_h=1 then return

/*breddepinner*/

mellom_b=bredde-(midtstokk*subtype)
