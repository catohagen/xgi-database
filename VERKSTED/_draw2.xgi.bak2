step=mellom_b / (inndel_pinner+1)

p1=step

pinner=inndel_pinner

do until antall_hpinne<=0
 do until pinner<=0
 StartPolyline p1 0
 EndLine p1 hoyde
 p1=p1+step
 pinner=pinner-1
 end
p1=p1+step+midtstokk
pinner=inndel_pinner
antall_hpinne=antall_hpinne-1
end

return




