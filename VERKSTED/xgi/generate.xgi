kapp=kapp/2
A2= (KAPP*KAPP) + (BUEH0YDE*BUEH0YDE)
R=A2 / (BUEH0YDE*2)
r=trunc(r, 3)
R=R*-1
R= "R"r

X=KAPP*2
x=x*-1
x= "X"|| x
if RAM=44 then ramme="22mm"
if RAM=32 then ramme="16mm"

/* GCODE TYPE E */
WRITEE:
call writeln(pfile,"(E BUE "KAPPB" "antall" STK)")
call writeln(pfile,"G0" ZAXE)
call writeln(pfile,G91)
call writeln(pfile,'G1 X-24')
