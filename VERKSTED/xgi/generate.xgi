call open(dat,cncpresetfile,'R')
garbage=readln(dat)
cnchoyde=readln(dat)
call close(dat)


/* open file , and keep open until eof */
call open(dat,jobbfile,'R')
garbage=readln(dat)
dato=readln(dat)
kundenr=readln(dat)
merk=readln(dat)
levuke=readln(dat)
garbage=readln(dat)

call hentkundeinfo

prognr=10
